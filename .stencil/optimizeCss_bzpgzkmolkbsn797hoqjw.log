:host {
  display: block;
}
:host .mdp-container {
  float: left;
  position: absolute;
  left: 0;
  top: 40px;
  width: 300px;
  min-height: 350px;
  background: #fff;
  -webkit-box-shadow: 10px 10px 40px rgba(0, 0, 0, 0.2);
  box-shadow: 10px 10px 40px rgba(0, 0, 0, 0.2);
  border-radius: 20px;
  overflow: hidden;
  padding: 25px 30px;
}